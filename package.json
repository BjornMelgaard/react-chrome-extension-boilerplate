{
  "name": "react-chrome-extension-boilerplate",
  "version": "0.9.0",
  "description": "Boilerplate for Chrome extension React.js project",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint:base": "eslint --ignore-path .eslintignore",
    "lint": "yarn run lint:base .",
    "lint:fix": "yarn run lint:base --fix .",
    "format:base": "prettier --config './.prettierrc.yaml' --ignore-path './.eslintignore' --write",
    "format": "yarn run format:base './**/*.{js,jsx,json}'",
    "format-fix": "yarn run format && yarn run lint:fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format:base",
      "yarn run lint:base --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhen0409/react-chrome-extension-boilerplate.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Jhen <developer@jhen.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.33",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.33",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.33",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.33",
    "@babel/preset-env": "^7.0.0-beta.33",
    "@babel/preset-react": "^7.0.0-beta.33",
    "@babel/register": "^7.0.0-beta.33",
    "@babel/runtime": "^7.0.0-beta.33",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-direct-import": "^0.6.0-beta.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-root-import": "^5.1.0",
    "chokidar": "^1.7.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "crx": "^3.0.3",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-babel-plugin-root-import": "^0.0.11",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "minimist": "^1.2.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.8.2",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.5",
    "write-file-webpack-plugin": "^4.2.0",
    "write-json-webpack-plugin": "^1.0.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "chrome-promise": "^2.1.1",
    "material-ui": "^1.0.0-beta.22",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.0.0",
    "ramda-asserters": "^0.0.12",
    "ramda-universal-trace": "^0.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-logic": "^0.12.3",
    "rxjs": "^5",
    "styled-components": "^2.2.4"
  }
}
